	"name": "the-run-club",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"docker:up": "docker-compose up -d",
		"docker:down": "docker-compose down",
		"db:migrate": "bunx drizzle-kit push --config=drizzle-dev.config.ts",
		"db:generate": "bunx drizzle-kit generate --config=drizzle-dev.config.ts",
		"db:migrate:prod": "NODE_ENV=production bunx drizzle-kit push --config=drizzle-prod.config.ts --verbose",
		"db:reset": "npx tsx src/db/reset.ts",
		"db:studio": "bunx drizzle-kit studio --config=drizzle-dev.config.ts",
		"dev": "next dev  --turbopack",
		"build": "next build",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"db:seed": "NODE_ENV=development bun run src/db/seed.ts",
		"db:seed:prod": "NODE_ENV=production bun run src/db/seed.ts",
		"db:seed-videos": "NODE_ENV=development bun run src/db/seed-videos.ts",
		"db:seed-videos:prod": "NODE_ENV=production bun run src/db/seed-videos.ts",
		"db:seed-clubs": "bun scripts/seed-clubs.ts",
		"db:load-clubs": "bun scripts/load-clubs-csv.ts",
		"db:load-clubs:prod": "NODE_ENV=production bun scripts/load-clubs-csv.ts",
		"update-colors": "bunx tsx scripts/update-channel-colors.ts",
		"worker:prod": "NODE_ENV=production bun run scripts/start-worker.ts",
		"queue:prod": "NODE_ENV=production bun run scripts/queue-episodes.ts",
		"worker": "NODE_ENV=development bun run scripts/start-worker.ts",
		"queue": "NODE_ENV=development bun run scripts/queue-episodes.ts",
		"sync:mentions": "bun run scripts/sync-athlete-mentions.ts",
		"check:supabase": "bun run scripts/check-supabase-config.ts",
		"process:episode": "NODE_ENV=production bun run scripts/process-episode.ts",
		"clear:queue": "bun run scripts/clear-queue.ts",
		"backfill:tags": "bun run scripts/backfill-tags.ts",
		"backfill:tags:prod": "NODE_ENV=production bun run scripts/backfill-tags.ts",
		"deploy:tagging-function": "./scripts/deploy-tagging-function.sh",
		"apply:tagging-triggers": "./scripts/apply-tagging-triggers.sh",
		"athlete-detection": "concurrently \"bun run worker\" \"bun run queue\"",
		"db:migrate:athletes": "bun run scripts/migrate-athletes.ts",
		"db:rollback:athletes": "bun run scripts/rollback-athletes.ts",
		"reprocess:mentions": "bun run scripts/reprocess-athlete-mentions.ts",
		"reprocess:mentions:prod": "NODE_ENV=production bun run scripts/reprocess-athlete-mentions.ts",
		"optimize:images": "NODE_ENV=production bun run scripts/optimize-content-images.ts",
		"test": "vitest"
